{
  "hosting": {
    "site": "pollution-eye",
    "public": "dist",
    "predeploy": "webpack --mode=production",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
